  "common_names": {
    "HIP 13847": [{"english": "Eridanus: The last star of the river, the bright one"}],
    "HIP 21421": [{"english": "[The stars in the face, called 'Hyades':] the bright star of the Hyades, the reddish one on the southern eye"}],
    "HIP 24436": [{"english": "Orion: The bright star in the left foot, which is [applied in] common to the water [of Eridanus]"}],
    "HIP 24608": [{"english": "The star on the left shoulder, called Capella"}],
    "HIP 27989": [{"english": "Orion: The bright, reddish star on the right shoulder"}],
    "HIP 30438": [{"english": "Argo: The more advanced of the 2 stars in the other [southern, rear] steeringoar, called Canopus"}],
    "HIP 32349": [{"english": "The star in the mouth, the brightest, which is called 'the Dog' and is sparkeling"}],
    "HIP 37279": [{"english": "The bright star just over the hindquarters, called Procyon"}],
    "HIP 49669": [{"english": "The star on the heart, called 'Regulus'"}],
    "HIP 57632": [{"english": "The star on the end of the tail [of the Lion]"}],
    "HIP 65474": [{"english": "The star on the left hand, called 'Spica'"}],
    "HIP 69673": [{"english": "The star between the thighs, called 'Arcturus', reddish"}],
    "HIP 71683": [{"english": "Centaurus: The star on the end of the right front leg"}],
    "HIP 91262": [{"english": "The bright star on the shell, called Lyra"}],
    "HIP 113368": [{"english": "Aqr: [The stars on the flow of the water:] The star at the end of the water and on the mouth of & PsA: The star in the mouth, which is the same as the beginning of the water"}]
  }
