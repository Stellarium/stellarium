"\t\t<td><b>Picture</b></td>\n"
"\t</tr>\n"
"\t<tr valign=\"top\">\n"
"\t\t<td><notr>UMi</notr></td>\n"
"\t\t<td><notr>&Mu;&iota;&kappa;&rho;&#942; &#702;&#714;&Alpha;&rho;&kappa;"
"&tau;&omicron;&sigmaf;</notr></td>\n"
"\t\t<td>Little She-Bear</td>\n"
"\t\t<td>missing</td>\n"
"\t</tr>\n"
