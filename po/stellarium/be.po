#: src/core/modules/NomenclatureItem.cpp:659
msgid "Fissure."
msgstr "Расколіна."

#. TRANSLATORS: Description for landform 'rupes'
#: src/core/modules/NomenclatureItem.cpp:663
msgid "Scarp."
msgstr "Зрыў."

