msgid "Planetary Mass, Mjup"
msgstr "Маса планети у масах Юпітера"

#: plugins/Exoplanets/src/gui/ExoplanetsDialog.cpp:771
msgid "Planetary Radius, Rjup"
msgstr "Радіус планети у радіусах Юпітера"

