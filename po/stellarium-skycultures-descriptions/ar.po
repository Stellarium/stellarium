"\t<td style=\"text-align:right;\">14°</td>\n"
"\t<td>6 أذرعة</td>\n"
"\t<td style=\"text-align:right;\">14°</td>\n"
"\t<td>6 أذرعة</td>\n"
"\t<td>رمح</td>\n"
"</tr>\n"
