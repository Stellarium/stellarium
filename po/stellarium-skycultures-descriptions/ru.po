msgstr ""

#. type: Content of: <ul><ul><li>
#: ../../skycultures/vanuatu_netwar/description.en.utf8:13
msgid "Waning crescent: Numanmel"
msgstr ""

#. type: Content of: <ul><ul><li>
#: ../../skycultures/vanuatu_netwar/description.en.utf8:14
