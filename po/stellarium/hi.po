msgid "6 Textures"
msgstr "6 बुनाईयाँ"

#: plugins/Scenery3d/src/gui/Scenery3dDialog_p.hpp:70
msgid "Cubemap"
msgstr "घनक्षेत्र मानचित्र"

#: plugins/Scenery3d/src/gui/Scenery3dDialog_p.hpp:72
msgid "Geometry shader"
msgstr "रेखाचित्र छायक"

