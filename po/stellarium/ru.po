#: src/core/StelObject.cpp:169
msgid "(apparent)"
msgstr "(видимые)"

#: src/core/StelObject.cpp:167 src/core/StelObject.cpp:169
#: src/core/StelObject.cpp:174 src/core/StelObject.cpp:176
#, qt-format
msgid "Az/Alt: %1/%2"
msgstr "Азимут/высота: %1/%2"

#: src/core/StelObject.cpp:189 src/core/StelObject.cpp:191
