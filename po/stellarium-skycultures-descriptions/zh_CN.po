"\t<td><notr>UMi</notr></td>\n"
"\t<td><notr>&Mu;&iota;&kappa;&rho;&#942; &#702;&#714;&Alpha;&rho;&kappa;&tau;&omicron;&sigmaf;</notr></td>\n"
"\t<td>Small She-Bear</td>\n"
"\t<td><notr>Ursa Minor</notr></td>\n"
"\t<td>like IAU constellation</td>\n"
"\t<td></td>\n"
"</tr>\n"
"<tr valign=\"top\">\n"
